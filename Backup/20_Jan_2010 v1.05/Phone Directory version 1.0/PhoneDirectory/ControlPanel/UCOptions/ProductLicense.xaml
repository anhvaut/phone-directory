<UserControl x:Class="ControlPanel.UCOptions.ProductLicense"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
     Height="313" Width="417" Loaded="UserControl_Loaded">
    <Grid>
        <TextBlock FontSize="12" FontWeight="Bold" Height="21" Margin="80,25,98,0" Name="textBlock1" Text="Product Licensing" TextAlignment="Center" VerticalAlignment="Top" /><TextBlock Height="21" Margin="34,60,98,0" Name="textBlock2" VerticalAlignment="Top" Text="Your product key (xxxxx-xxxxx-xxxxx-xxxxx-xxxxx)" /><TextBox Height="23" Margin="36,81,98,0" Name="txtProductKey" VerticalAlignment="Top" TextChanged="txtProductKey_TextChanged" MaxLength="29" /><Button Height="23" HorizontalAlignment="Left" Margin="36,115,0,0" Name="btnActivate" VerticalAlignment="Top" Width="75" Click="btnActivate_Click" IsEnabled="False">Activate</Button><TextBlock Margin="36,0,32,117" Name="lbLicensingStatus" Text="lbLicensingStatus" Visibility="Hidden" FontSize="12" FontWeight="Bold" Height="21" VerticalAlignment="Bottom" /><TextBlock Margin="36,0,32,89" Name="lbLicenseType" Text="lbLicenseType" Visibility="Hidden" Height="21" VerticalAlignment="Bottom" FontSize="12" /><TextBlock Height="21" Margin="36,0,22,59" Name="lbExpireDate" Text="lbExpireDate" VerticalAlignment="Bottom" Visibility="Hidden" FontSize="12" /><Image Height="37" HorizontalAlignment="Right" Margin="0,76,32,0" Name="imgFail" Source="/ControlPanel;component/Images/fail.jpg" Stretch="None" ToolTip="Invalid domain" VerticalAlignment="Top" Visibility="Hidden" Width="49" /><Image Height="37" HorizontalAlignment="Right" Margin="0,76,32,0" Name="imgValid" Source="/ControlPanel;component/Images/check.png" Stretch="None" ToolTip="OK" VerticalAlignment="Top" Visibility="Hidden" Width="49" /></Grid>
</UserControl>
